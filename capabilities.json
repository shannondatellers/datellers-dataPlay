          "sortBy": { "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "for": {
            "in": "category"
          },
          "dataReductionAlgorithm": {
            "top": {
              "count": 30000
            }
          }
        },
        "values": {
          "select": [
            {
              "for": {
                "in": "sortBy"
              }
            }
          ],
          "dataReductionAlgorithm": {
            "top": {
              "count": 30000
            }
          }
        }
      }
    }
